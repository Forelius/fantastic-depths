<div>
   <div class="attack-desc">{{description}}</div>
   {{#if toHitResult.message}}
   <div class="attack-result">{{{toHitResult.message}}}</div>
   {{/if}}
</div>
<div class="bordered" style="padding-bottom:2px;">
   <div class="flexrow collapser">
      <img class="item-image" src='{{item.img}}' data-tooltip='{{item.name}}' />
      <div class="item-name">{{item.name}}</div>
   </div>
   <div class="collapsible-content collapsed">
      {{{item.system.description}}}
   </div>
</div>
{{#if durationMsg}}
<div class="spell-duration" style="margin-bottom:2px;">
   {{durationMsg}}
</div>
{{/if}}
{{#if rollContent}}
{{{rollContent}}}
{{/if}}
<div name="digest" class="digest">
   {{#each digest as |ditem|}}
   <div>{{ditem}}</div>
   {{/each}}
</div>
{{> "rollSaveBtn"}}
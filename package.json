{
   "name": "fantastic-depths",
   "version": "2.0.0",
   "description": "CSS compiler for the fade system",
   "type": "module",
   "scripts": {
      "clean": "rimraf ./module",
      "build": "npm run clean && rollup -c",
      "buildd": "npx tsc",
      "buildcss": "sass src/scss/fantastic-depths.scss css/fantastic-depths.css --style=expanded --no-source-map",
      "watchcss": "sass src/scss/fantastic-depths.scss css/fantastic-depths.css --style=expanded --source-map --watch",
      "publish": "--echo git tag <version> && git push origin main && git push origin <version>",
      "lint": "eslint ."
   },
   "browserslist": [
      "last 3 versions"
   ],
   "author": "forelius",
   "license": "MIT",
   "private": true,
   "devDependencies": {
      "@eslint/js": "^9.39.2",
      "@eslint/json": "^0.14.0",
      "@rollup/plugin-terser": "^0.4.4",
      "@rollup/plugin-typescript": "^12.3.0",
      "eslint": "^9.39.2",
      "eslint-plugin-import": "^2.32.0",
      "globals": "^16.5.0",
      "prettier": "^3.6.2",
      "rimraf": "^6.1.2",
      "rollup": "^4.54.0",
      "sass": "^1.53.0",
      "tslib": "^2.8.1",
      "typescript": "^5.9.3",
      "typescript-eslint": "^8.50.0"
   }
}

{
   "name": "fantastic-depths",
   "version": "2.0.0",
   "description": "CSS compiler for the fade system",
   "scripts": {
      "build": "sass src/scss/fantastic-depths.scss css/fantastic-depths.css --style=expanded --no-source-map",
      "watch": "sass src/scss/fantastic-depths.scss css/fantastic-depths.css --style=expanded --source-map --watch",
      "publish": "--echo git tag <version> && git push origin main && git push origin <version>"
   },
   "browserslist": [
      "last 3 versions"
   ],
   "author": "forelius",
   "license": "MIT",
   "private": true,
   "devDependencies": {
      "sass": "^1.53.0"
   }
}